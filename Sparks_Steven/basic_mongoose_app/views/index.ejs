<html>
<head>
<title></title>
</head>
<body>
    <form method="post" action="/users">
        <p>
        <label>Name</label>
        <input type="text" name="name">
        </p>
        <p>
        <label>Age</label>
        <input type="integer" name="age">
        </p>
        <input type="submit" name="submit">
    </form>
    <% for (x in users) { %>
        <p><%= users[x].name %> <%= users[x].age %></p>
    <% } %>
</body>
</html>
 